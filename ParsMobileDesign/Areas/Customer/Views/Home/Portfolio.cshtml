        @foreach (var item in Model)
            {
                var path=item.Images.FirstOrDefault().ImageAddr;
                <div class="col-md-3 col-sm-6 col-padding text-center">
                <div class="work">
                    <img src="@path" class="image-popup" alt="ads" />
                    @*<a href="#" style="background-image:@str">*@
